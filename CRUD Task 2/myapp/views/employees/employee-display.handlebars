<html>
    <h1>Employee Table</h1>
    <table border="1px">
        <tr>
              <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Company Name</th>

        <th>Designation</th>
        <th>Contact number</th>
        </tr>
      
     {{#each mydata}}
     <tr>
         <td></td>
         <td>{{first_name}}</td>
         <td>{{last_name}}</td>
         <td>{{email}}</td>
         <td>{{company_name}}</td>
         <td>{{designation}}</td>
         <td>{{contact_number}}</td>
          <td>
        <a href="/employees/edit/{{_id}}">Edit</a> |
        <a href="/employees/delete/{{_id}}">Delete</a>

    </td>
     </tr>
     {{/each}}
    </table>
</html>